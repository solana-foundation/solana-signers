{
    "name": "@solana-signers/test-utils",
    "author": "Solana Foundation",
    "license": "MIT",
    "repository": "https://github.com/solana-foundation/solana-signers",
    "version": "0.1.0",
    "description": "Shared testing utilities for Solana signer integration tests",
    "keywords": [
        "solana",
        "testing",
        "integration-tests",
        "litesvm"
    ],
    "type": "module",
    "sideEffects": false,
    "private": true,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsc --build",
        "clean": "rm -rf dist *.tsbuildinfo",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@solana-program/memo": "^0.8.0",
        "@solana-signers/core": "workspace:*",
        "@solana/kit": "^4.0.0",
        "litesvm": "^0.3.3"
    },
    "devDependencies": {
        "@types/node": "^24.7.1"
    }
}
